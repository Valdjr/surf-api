{
  "name": "surf-api",
  "version": "1.0.0",
  "description": "typescript node js surf api",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js | pino-pretty -c",
    "start:local": "ts-node-dev --respawn --transpile-only 'src/index.ts' | pino-pretty -c",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api",
    "surf"
  ],
  "author": "valdjr",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-pino-logger": "^4.0.2",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.4",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.6.2",
    "@types/pino": "^6.3.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "nock": "^13.0.4",
    "pino-pretty": "^4.2.0",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.15",
    "express-pino-logger": "^5.0.0",
    "http-status-codes": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.2",
    "pino": "^6.5.1",
    "swagger-ui-express": "^4.1.4"
  }
}
